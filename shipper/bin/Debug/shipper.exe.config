<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
      <section name="Processors" type="shipper.Configuration.ProcessorsSection,shipper"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="" value=""/>
  </appSettings>
  <Processors>
    <InputProcessors>
      <InputProcessor type="udp" metadata="22000" output="redis"/>
      <InputProcessor type="udp" metadata="22001" output="redis"/>
    </InputProcessors>
    <OutProcessors>
      <!-- metadata="connectionstring|db|key" -->
      <OutputProcessor name="redis" type="redis" metadata="redis-logstash.aws.csprd.com.au,connectTimeout=3000,abortConnect=true,connectRetry=3,KeepAlive=180|6|logstash"/>
    </OutProcessors>
  </Processors>
</configuration>